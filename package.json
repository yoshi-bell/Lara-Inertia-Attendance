        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.563.0",
        "react-day-picker": "^9.13.2",
        "tailwind-merge": "^3.4.0",
        "tailwindcss-animate": "^1.0.7"
    }
}
